# -----------------------------
# KKV DEV ENV .profile settings
# -----------------------------
echo -n "setting DEV_ENV profile from github/KKV_DEV_ENV/profile ..."
export HISTSIZE=50000
export PATH=$PATH:.
export PS1='$(logname)@$(hostname -s):$PWD % '
#export PS1=$'\E[34;7m$(logname)@$(hostname -s)\E[0m:\E[32;1m$PWD\E[0m$ '
#export PS1='\e[33;1m\u@\h: \e[31m\W\e[0m\$ '
alias vi='vim'

set -o vi
echo "done"

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/opt/mqm/lib64:/opt/mqm/lib
